
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, user-scalable=no">
    <meta name="description" content="">
    <meta name="Copyright" content="宜联科技" />
    <meta name="author" content="">

    <meta http-equiv="refresh" content="3600">

    <link rel="shortcut icon" href="../images/favicon/favicon.png" type="image/x-icon">
    <link rel="icon" href="../images/favicon/favicon.png" type="image/x-icon">

    <title>@(Model.sJobName)-遂宁市安居区法院</title>

    <link href="~/Areas/Mobile/Content/JudgeStudio/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/Mobile/Content/JudgeStudio/css/normalize.css" rel="stylesheet" />
    <link href="~/Areas/Mobile/Content/JudgeStudio/css/tjs.css" rel="stylesheet" />

    <link href="~/Areas/Mobile/Content/JudgeStudio/css/juds.css" rel="stylesheet" />
</head>
@using EFModels.MyModels;
@model ViewJudgeStudio
<body data-spy="scroll">
    <div class="headbox">
        <div class="header">
            <div class="fyname row text-left vertical-middle-sm">
                <div class="col-sm-8 text-left">
                    <h1>遂宁市安居区人民法院</h1>
                    <p class="lead">诉讼服务中心——法官工作室</p>
                </div>
            </div>
        </div>
    </div>
    <div class="pagebg">
        <div class="page">
            @if (Model.IsBusy)
            {
            <p class="title_big">@Model.sJobName（正在调解......）</p>
            }
            else{
            <p class="title_big">@Model.sJobName（空闲中......）</p>
            }
            <div class="myContent">
                <div class="my_content_box">    
                    <p class="name_title">调解员：@Model.RealName</p>
                    <div class="img_box">  
                        @*<img src="/Areas/Mobile/Content/JudgeStudio/images/tcmb_03.png">*@  
                        <img src="@Model.UserPic">  
                    </div>
                    @if (Model.IsBusy)
                    {
                     <p class="name_title">调节事项</p>
                    }
                    else
                    {
                        <p class="name_title">电话：@Model.Mobile</p>
                    }
                    @if (Model.IsBusy)
                    {
                    <div class="content_box">
                        <div style="min-height:100px;">
                            @Html.Raw(Model.sCaseIntroduce)
                        </div>
                    </div>
                    }
                </div>
            </div>
        </div>
    </div>
    <div class="footer divbgtransparent_white7"><div class="copyrights"></div></div>
</body>
</html>
